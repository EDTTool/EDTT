     <text:p text:style-name="P27">Transport DUT side API:</text:p>
     <text:p text:style-name="P29">start(), stop()</text:p>
     <text:p text:style-name="P29">read(u8_t *pbuffer, size_t count, int flags)</text:p>
     <text:p text:style-name="Frame_20_contents"><text:span text:style-name="T28">write(u8_t *pbuffer, size_t count, int flags)</text:span></text:p>
     <draw:enhanced-geometry draw:mirror-horizontal="false" draw:mirror-vertical="false" svg:viewBox="0 0 0 0" draw:text-areas="0 0 ?f3 ?f2" draw:type="ooxml-rect" draw:enhanced-path="M 0 0 L ?f3 0 ?f3 ?f2 0 ?f2 Z N">
      <draw:equation draw:name="f0" draw:formula="logwidth/2"/>
      <draw:equation draw:name="f1" draw:formula="logheight/2"/>
      <draw:equation draw:name="f2" draw:formula="logheight"/>
